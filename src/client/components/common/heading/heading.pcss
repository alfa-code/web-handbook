.h1 {
    font-size: var(--text-heading-1);
}

.h2 {
    font-size: var(--text-heading-2);
}

.h3 {
    font-size: var(--text-heading-3);
}

.h4 {
    font-size: var(--text-heading-4);
}

.h5 {
    font-size: var(--text-heading-5);
}

.h6 {
    font-size: var(--text-heading-6);
}

.h1, .h2, .h3, .h4, .h5, .h6 {
    margin-top: 0;
    font-family: Rubik;
    font-style: normal;
    font-weight: bold;
    color: var(--text-primary);
    margin-bottom: 0.6em;
}
